{
    "name": "mediagone/small-uid-doctrine",
    "type": "library",
    "description": "Provides Doctrine types for mediagone/small-uid package.",
    "keywords": ["doctrine", "uid", "uuid", "ulid", "guid", "primary key", "entity", "database", "type"],
    "license": "MIT",
    "authors": [
        {"name": "Bruce Suire", "role": "Developer"}
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Mediagone\\SmallUid\\Doctrine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Mediagone\\SmallUid\\Doctrine\\": "tests/"
        }
    },
    "require": {
        "php-64bit": "^7.4|^8.0",
        "ext-gmp": "*",
        "doctrine/dbal": "^2.7",
        "mediagone/small-uid": "^0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "tests": "phpunit -c phpunit.xml.dist --testsuite Unit_Tests"
    }
}
